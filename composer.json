{
    "name": "sudippalash/role-creator",
    "description": "Laravel Package for Role Management. This package uses the Spatie Laravel Permission package and includes role CRUD functionality & Seeder for permission.",
    "keywords": [
        "Sudip",
        "Laravel Role Management",
        "Role Management",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudip Palash",
            "email": "palash.sudip@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "spatie/laravel-permission": "^6.0"
    },
    "autoload": {
        "psr-4": {
          "Sudip\\RoleCreator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sudip\\RoleCreator\\Providers\\AppServiceProvider",
                "Sudip\\RoleCreator\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
